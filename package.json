{
  "name": "@srclaunch/http-server",
  "description": "Simple API wrapper around Express server",
  "author": "Steven Bennett <steven@srclaunch.com>",
  "license": "MIT",
  "version": "0.1.4",
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "package.json",
    "dist"
  ],
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "scripts": {
    "build:types": "tsc --build --force",
    "build:applab": "yarn applab build",
    "build": "yarn clean && yarn build:applab && yarn build:types",
    "clean": "rm -fr ./dist",
    "lint:eslint": "yarn eslint ./src --ext js,ts,tsx --max-warnings 0",
    "lint:stylelint": "yarn stylelint '**/*.ts[x]'",
    "lint": "yarn lint:eslint && yarn lint:stylelint",
    "push": "git push --follow-tags origin main",
    "release": "yarn standard-version && git push --follow-tags origin main",
    "test": "ava",
    "test:watch": "yarn ava --watch",
    "test:coverage": "yarn ava --coverage",
    "test:coverage:typescript": "yarn typescript-coverage-report",
    "version:push": "git push --follow-tags origin main",
    "version:bump": "yarn version patch --force",
    "version:release": "yarn standard-version",
    "yui": "yarn upgrade-interactive"
  },
  "devDependencies": {
    "@srclaunch/cli": "^0.1.18",
    "@srclaunch/dx": "^0.1.6",
    "@srclaunch/types": "^0.1.3",
    "@types/async-exit-hook": "^2.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.28",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/keygrip": "^1.0.2",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "ava": "^4.1.0",
    "esbuild": "^0.14.19",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-stylelint": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.2.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-regexp": "^1.5.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-sort-requires": "^2.1.0",
    "eslint-plugin-testing-library": "^5.0.5",
    "eslint-plugin-unicorn": "^40.1.0",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "standard-version": "^9.3.2",
    "stylelint": "^14.3.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@srclaunch/exceptions": "^0.1.6",
    "@srclaunch/logger": "^0.1.2",
    "@srclaunch/node-environment": "^0.1.2",
    "async-exit-hook": "^2.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "helmet": "^5.0.2",
    "multer": "^1.4.4"
  }
}
